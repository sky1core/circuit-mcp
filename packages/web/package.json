{
  "name": "@sky1core/circuit-web",
  "version": "0.0.14",
  "description": "Web automation CLI using Playwright for Snowfort Circuit",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "circuit-web": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format esm,cjs --dts --clean --sourcemap --external @modelcontextprotocol/sdk --external playwright-core --external commander",
    "clean": "rm -rf dist",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "playwright-core": "^1.40.0",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0"
  },
  "license": "Apache-2.0",
  "author": "sky1core (forked from Snowfort LLC)",
  "repository": {
    "type": "git",
    "url": "https://github.com/sky1core/circuit-mcp.git",
    "directory": "packages/web"
  }
}
